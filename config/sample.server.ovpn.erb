client

dev tun

proto <%= server.protocol %>

remote <%= server.hostname %> <%= server.port %>

resolv-retry infinite
nobind
persist-key
persist-tun
reneg-sec 0

auth-user-pass
comp-lzo

<ca>
<%= server.server_crt %>
</ca>

<cert>
<%= server.client_crt %>
</cert>

<key>
<%= server.client_key %>
</key>
